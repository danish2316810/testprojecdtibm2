{
  "name": "testprojectibm",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8",
    "express": "^4",
    "@sap/xssec": "^4",
    "@cap-js/hana": "^1"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-actionui": "cds watch --open actionui/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy testprojectibm --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-list-report-for-internalisation": "cds watch --open list-report-for-internalisation/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-app-dan-contracterrors": "cds watch --open app-dan-contracterrors/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "db": "hana",
      "connectivity": true,
      "destinations": true,
      "html5-runtime": true
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "sapux": [
    "app/list-report-for-internalisation",
    "app/app-dan-contracterrors",
    "app/app-dan-contracterrors"
  ]
}
